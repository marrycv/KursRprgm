\end_layout

\begin_layout Plain Layout

res <- integer(K)
\end_layout

\begin_layout Plain Layout

for (k in 1:K){
\end_layout

\begin_layout Plain Layout

res[k] <- sum(sample(1:6,size=N, replace=TRUE))
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout

return(res)}
